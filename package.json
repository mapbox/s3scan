{
  "name": "@mapbox/s3scan",
  "version": "2.0.0",
  "description": "Streaming operations on S3 objects",
  "main": "dist/lib/index.js",
  "engines": {
    "node": "^6.0.0"
  },
  "scripts": {
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "npx tsc",
    "test": "eslint lib index.js && tape test/*.test.js",
    "docs": "documentation build -f md > api.md"
  },
  "bin": {
    "s3purge": "dist/bin/s3Purge.js",
    "s3keys": "dist/bin/s3Keys.js",
    "s3scan": "dist/bin/s3Scan.js"
  },
  "author": "Mapbox",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/s3scan.git"
  },
  "bugs": {
    "url": "https://github.com/mapbox/s3scan/issues"
  },
  "homepage": "https://github.com/mapbox/s3scan#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.408.0",
    "aws-sdk": "^2.49.0",
    "commander": "^11.0.0",
    "figlet": "^1.6.0",
    "minimist": "^1.2.0",
    "parallel-stream": "^1.1.2",
    "s3urls": "^1.5.2",
    "split": "^1.0.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/node": "^20.5.9",
    "d3-queue": "^3.0.7",
    "documentation": "^8.0.2",
    "eslint": "^5.2.0",
    "eslint-config-unstyled": "^1.1.0",
    "tape": "^4.6.3",
    "underscore": "^1.8.3"
  }
}
